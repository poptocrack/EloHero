{
  "expo": {
    "name": "EloHero",
    "slug": "elohero",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android"],
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elohero.app",
      "infoPlist": {
        "CFBundleDisplayName": "EloHero",
        "CFBundleLocalizations": ["en", "fr"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.elohero.app",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "fr"],
            "android": ["en", "fr"]
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "elohero"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "firebaseApiKey": "AIzaSyASlpzKNXuR9X8jnRjmZd4Sq4kQwBQs7JI",
      "firebaseAuthDomain": "elohero.firebaseapp.com",
      "firebaseProjectId": "elohero",
      "firebaseStorageBucket": "elohero.firebasestorage.app",
      "firebaseMessagingSenderId": "100850883969",
      "firebaseAppId": "1:100850883969:web:f9e4f7af24d7cb8ce25c80",
      "eas": {
        "projectId": "5ad4fa89-03e8-4240-945b-a2f6c2e8237b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5ad4fa89-03e8-4240-945b-a2f6c2e8237b",
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 3000
    }
  }
}
