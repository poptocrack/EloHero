{
  "name": "elohero",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "./start-dev.sh && expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:android": "eas build -p android --profile production --auto-submit",
    "build:ios": "eas build -p ios --profile production --auto-submit",
    "emulators": "firebase emulators:start --only firestore,functions",
    "emulators:all": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:import": "firebase emulators:start --import ./emulator-data"
  },
  "dependencies": {
